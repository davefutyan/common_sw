<group description="Exposure">
	<keyword name="T_STRT_O" data_type="real"     unit="sec" comment="OBT of the first measurement"/>
	<keyword name="T_STOP_O" data_type="real"     unit="sec" comment="OBT of the last measurement"/>
	<keyword name="NEXP"     data_type="integer"             comment="Number of co-added measurements"/>
	<keyword name="EXPTIME"  data_type="integer"  unit="ms"  comment="Exposure time of the individual exposures"/>
	<keyword name="TEXPTIME" data_type="integer"  unit="ms"  comment="Total exposure time of stacked images"/>
</group>